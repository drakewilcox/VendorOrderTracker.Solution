@{
  Layout = "_Layout";
}
@using VendorOrderTracker.Models; 

<h3>All Current Orders for this Vendor</h3>

<ol>
  @foreach (Order order in @Model["orders"])
  {
    <li> <a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.Title</a></li>
  }
</ol>

<p> <a href='/vendors/@Model["vendor"].Id/orders/new'>Add a New Order to Vendor</a></p>
<p> <a href='/vendors'>Return to Vendor List</a></p>